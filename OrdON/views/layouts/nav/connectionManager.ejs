<% if (locals.user) { %>
    <% if (locals.user.type === "patient") { %>
        <%- include('./navConnected'); -%>
    <% } else if (locals.user.type === "médecin") {%> 
        <%- include('./navConnectedMed'); -%>
    <% } else if (locals.user.type === "pharmacien") {%> 
        <%- include('./navConnectedPhar'); -%>
    <% } else { %> 
        <%- include('./navSimple'); -%>
    <% } %>   
<% } else { %>
    <%- include('./nav'); -%>
<% } %> 
<% if (locals.flash) { %> 
    <% if (locals.flash.success) { %> 
        <div class="sectiongreen flash">Compte validé avec succès</div>
    <% } else if(locals.flash.error) { %> 
        <div class="sectionred flash">Compte non valide</div>
    <% } %> 
<% } %> 
